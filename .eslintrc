{
  "rules": {
    "indent": [1, "tab"],
    "no-extra-bind": 1,
    "no-magic-numbers": 1,

    "quotes": [2, "double"],
    "linebreak-style": [2, "unix"],
    "semi": [2, "always"],
    "no-unused-vars": [2, {"args": "after-used", "varsIgnorePattern": ""}],
    "strict": [2,"global"],
    "eqeqeq": [2,"smart"],
    "curly": [2, "all"],
    "no-lonely-if": 2,
    "no-unused-expressions": 2,
    "block-scoped-var": 2,
    "complexity": [2, 7],
    "consistent-return": 2,
    "dot-notation": [2, { "allowKeywords": true }],
    "no-alert": 2,
    "no-case-declarations": 2,
    "no-else-return": 2,
    "no-empty-label": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-implied-eval": 2,
    "no-invalid-this": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 2,
    "no-native-reassign": 2,
    "no-return-assign": 2,
    "no-sequences": 2,
    "no-useless-call": 2,
    "no-with": 2,
    "wrap-iife": [2, "outside"]
  },
  "env": {
    "es6": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "ecmaFeatures": {
    "modules": true,
    "experimentalObjectRestSpread": true
  },
  "plugins": [
  ]
}